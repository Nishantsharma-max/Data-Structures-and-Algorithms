   "source": [
    "def BubbleSort(lst):\n",
    "     for j in range(len(lst)-1,-1,-1):\n",
    "          for i in range(j):\n",
    "               if lst[i]>lst[i+1]:\n",
    "                    lst[i],lst[i+1]=lst[i+1],lst[i]\n",
    "     print(lst)\n",
    "\n",
    "\n",
    "BubbleSort([8,5,3,4,7,2,7])\n"
   ]
  }
